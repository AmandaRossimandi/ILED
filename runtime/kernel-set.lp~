initiatedAt(driving_style(id75,bus,unsafe),474) :- 
        happensAt(abrupt_deceleration_start(id75,bus,very_abrupt),474),
        holdsAt(punctuality(id75,bus,punctual),474),
        holdsAt(passenger_density(id75,bus,high),474),
        holdsAt(passenger_satisfaction(id75,bus,reducing),474).

initiatedAt(driving_style(id75,bus,unsafe),478) :- 
        happensAt(noise_level_change(id75,bus,low),478),
        happensAt(sharp_turn_start(id75,bus,very_sharp),478),
        holdsAt(punctuality(id75,bus,punctual),478),
        holdsAt(passenger_density(id75,bus,high),478),
        holdsAt(passenger_satisfaction(id75,bus,reducing),478).

terminatedAt(driving_style(id75,bus,unsafe),475) :- 
        happensAt(abrupt_deceleration_end(id75,bus,very_abrupt),475),
        holdsAt(abrupt_deceleration(id75,bus,very_abrupt),475),
        holdsAt(driving_style(id75,bus,unsafe),475),
        holdsAt(punctuality(id75,bus,punctual),475),
        holdsAt(passenger_satisfaction(id75,bus,reducing),475),
        holdsAt(passenger_density(id75,bus,high),475).

terminatedAt(driving_style(id75,bus,unsafe),479) :- 
        happensAt(sharp_turn_end(id75,bus,very_sharp),479),
        holdsAt(sharp_turn(id75,bus,very_sharp),479),
        holdsAt(driving_style(id75,bus,unsafe),479),
        holdsAt(punctuality(id75,bus,punctual),479),
        holdsAt(passenger_satisfaction(id75,bus,reducing),479),
        holdsAt(passenger_density(id75,bus,high),479).

terminatedAt(driving_style(id75,bus,unsafe),472) :- 
        happensAt(abrupt_deceleration_end(id75,bus,very_abrupt),472),
        happensAt(my_stop_enter(id75,bus,stopId,early),472),
        holdsAt(internal_temperature(id75,bus,very_cold),472),
        holdsAt(noise_level(id75,bus,high),472),
        holdsAt(passenger_density(id75,bus,high),472),
        holdsAt(abrupt_deceleration(id75,bus,very_abrupt),472),
        holdsAt(punctuality(id75,bus,punctual),472),
        holdsAt(driving_style(id75,bus,unsafe),472),
        holdsAt(passenger_satisfaction(id75,bus,reducing),472).